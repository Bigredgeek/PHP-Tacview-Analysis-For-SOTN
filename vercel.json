{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "outputDirectory": "public",
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "redirects": [
    {
      "source": "/debriefing.php",
      "destination": "/api/debriefing",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/debriefing.php",
      "destination": "/api/debriefing"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}